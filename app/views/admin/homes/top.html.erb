<h2>注文履歴一覧</h2>
<table>
  <thead>
    <tr>
      <th>購入日時</th>
      <th>購入者</th>
      <th>注文個数</th>
      <th>注文ステータス</th>
    </tr>
    <tbody>
      <% @oders each do |oder| %>
      <tr>
        <td><%= oder.created_at %></td>
        <td><%= oder.user.name %></td>
        <td><%= oder.</td>
        <td></td>
      </tr>
    </tbody>
  </thead>
</table>